a = int(input("Enter first number : "))
b = int(input("Enter second number : "))
c = int(input("Enter third number : "))
if a==b:
    print("a and b are same")
elif b==c:
    print("b and c are same")
elif c==a:
    print("c and a are same")
else:
    if a > b:
        if a > c:
            print("a is greatest")
        else:
            print("c is greatest")
    elif b > c:
        if b > a:
            print("b is greatest")
        else:
            print("a is greatest")
    elif c > b:
        if c > a:
            print("c is greatest")
        else:
            print("a is greatest")
