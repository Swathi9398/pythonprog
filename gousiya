a=int(input("Enter the numb"))
b=int(input("Enter the numb"))
c=int(input("Enter the num"))
if (a==b):
  print(a,"is equal to",b)
elif(b==c):
  print(b,"is equal to",c)
elif(a==c):
  print(a,"is equal to",c)
if (a>b and a>c):
    print(a,"is greater")
elif (a<b and b>c):
    print(b,"is greater")
else:
  print(c,"is greater")
