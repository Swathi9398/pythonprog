a=int(input("Enter a value:"))
b=int(input("Enter b value:"))
c=int(input("Enter c value:"))
if a=b :
  print("both are equal")
elif b=c:
  print("Both are equal")
elif c=a:
  print("Both are equal")
else: 
  print("No two numbers are equal")
if(a>b and a>c):
  print("a is greater")
elif b>c :
  print("b is greater")
else:
  printf("c is greater")
